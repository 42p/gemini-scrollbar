{
  "private": true,
  "name": "gemini-scrollbar-gh-page",
  "scripts": {
    "start": "npm i --global postcss-cli autoprefixer less watch browserify watchify live-reload",
    "server": "python -m SimpleHTTPServer",
    "js:browserify": "browserify src/js/app.js -o build/bundle.js",
    "js:watch": "watchify src/js/app.js -o build/bundle.js -v",
    "css:prefix": "postcss --use autoprefixer -b 'last 2 versions' build/bundle.css",
    "css:less": "lessc src/less/app.less build/bundle.css",
    "css:watch": "watch 'npm run css:less && npm run css:prefix' src/less",
    "css:build": "lessc -x src/less/app.less build/bundle.css",
    "livereload": "live-reload --port 9091 build/",
    "build": "npm run css:build && npm run js:browserify"
  },
  "author": "Noel Delgado <auhcsei@gmail.com> (http://pixelia.me/)",
  "license": "MIT",
  "dependencies": {
    "gemini-scrollbar": "^1.4.1",
    "sanitize.css": "^1.1.0"
  }
}
